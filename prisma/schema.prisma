generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Documents {
  id         String   @id @default(cuid())
  name       String?
  url        String
  created_at DateTime @default(now())
}

model ChatHistory {
id         String   @id @default(cuid())
childId    String
title      String?
messages   Json
chatType   String   @default("parenthoodGPT")
created_at DateTime @default(now())
updated_at DateTime @default(now())
}

model documents {
  id        BigInt                 @id @default(autoincrement())
  content   String?
  metadata  Json?
  embedding Unsupported("vector")?
}
